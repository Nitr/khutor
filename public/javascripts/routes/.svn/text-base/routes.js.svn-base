var AppRouter = Backbone.Router.extend({
	routes: {
		"": 				 "root",
		"/trackers": 		 "root",
	    "/tracker/:id": 	 "editTrackers",
	    "/tracker/:id/view": "viewTracker",
	},
	initialize: function() {
	},
	root: function() {
		new trackersView();
	},
	editTrackers: function(id) {
		new trackerEditView({id: id});
	},
	viewTracker: function(id) {
		new trackView({id: id});
	}
});

